&CONTROL
    calculation = 'vc-relax'
    pseudo_dir = './pseudo'
    outdir = './outdir'
    tstress = .true.
    tprnfor = .true.
    disk_io = 'none'
    nstep = 300
    etot_conv_thr = 1e-05
    forc_conv_thr = 0.0001
/ 
&SYSTEM
    ecutwfc = 60
    ecutrho = 720
    occupations = 'smearing'
    smearing = 'gaussian'
    degauss = 0.01
    ntyp = 5
    nat = 104
    ibrav = 0
/ 
&ELECTRONS
    diagonalization = 'david'
    electron_maxstep = 120
    mixing_mode = 'local-TF'
    mixing_beta = 0.2
    mixing_ndim = 10
    conv_thr = 1e-06
/ 
&IONS
    ion_dynamics = 'bfgs'
/ 
&CELL
    cell_dynamics = 'bfgs'
    press_conv_thr = 1
/ 
